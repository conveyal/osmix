{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@osmix/pbf",
	"description": "A low level, modern, runtime agnostic OSM PBF parser and writer written in TypeScript.",
	"version": "0.0.7",
	"type": "module",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/conveyal/osmix.git"
	},
	"main": "./src/index.ts",
	"scripts": {
		"bench": "bun test --bench",
		"build": "tsc -p tsconfig.build.json",
		"release": "bun run build && bun pm pack --quiet && npm publish *.tgz --access=public --provenance=true",
		"test": "bun test",
		"typecheck": "tsgo --noEmit"
	},
	"devDependencies": {
		"@osmix/shared": "workspace:*",
		"@types/bun": "catalog:",
		"typescript": "catalog:"
	},
	"dependencies": {
		"pbf": "catalog:"
	}
}
