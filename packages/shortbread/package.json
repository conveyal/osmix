{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@osmix/shortbread",
	"type": "module",
	"version": "0.0.9",
	"description": "Shortbread schema vector tile encoder for Osmix",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/conveyal/osmix.git"
	},
	"exports": {
		".": {
			"import": "./src/index.ts",
			"default": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"release": "bun run build && bun pm pack --quiet && npm publish *.tgz --access=public --provenance=true",
		"test": "bun test",
		"typecheck": "tsgo --noEmit"
	},
	"dependencies": {
		"@osmix/core": "workspace:*",
		"@osmix/shared": "workspace:*",
		"@osmix/vt": "workspace:*"
	},
	"devDependencies": {
		"@mapbox/tilebelt": "^2.0.3",
		"@mapbox/vector-tile": "^2.0.4",
		"@types/bun": "catalog:",
		"pbf": "catalog:",
		"typescript": "catalog:"
	}
}
