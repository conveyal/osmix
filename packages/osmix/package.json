{
	"$schema": "https://json.schemastore.org/package",
	"name": "osmix",
	"type": "module",
	"version": "0.1.6",
	"description": "Full OSM toolkit for JavaScript and TypeScript",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"bun": "./src/index.ts",
			"development|production": "./src/index.ts",
			"default": "./dist/index.js"
		}
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/conveyal/osmix.git"
	},
	"homepage": "https://github.com/conveyal/osmix#readme",
	"bugs": {
		"url": "https://github.com/conveyal/osmix/issues"
	},
	"sideEffects": false,
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"prepare": "bun run build",
		"release": "bun publish",
		"test": "bun test",
		"typecheck": "tsgo --noEmit"
	},
	"devDependencies": {
		"@mapbox/tilebelt": "^2.0.3",
		"@types/bun": "catalog:",
		"@types/geojson": "catalog:",
		"typescript": "catalog:"
	},
	"dependencies": {
		"@osmix/change": "workspace:*",
		"@osmix/core": "workspace:*",
		"@osmix/geojson": "workspace:*",
		"@osmix/geoparquet": "workspace:*",
		"@osmix/gtfs": "workspace:*",
		"@osmix/json": "workspace:*",
		"@osmix/pbf": "workspace:*",
		"@osmix/raster": "workspace:*",
		"@osmix/router": "workspace:*",
		"@osmix/shapefile": "workspace:*",
		"@osmix/shared": "workspace:*",
		"@osmix/vt": "workspace:*",
		"comlink": " ^4.4.2",
		"dequal": "catalog:"
	},
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"src",
		"README.md",
		"LICENSE"
	]
}
