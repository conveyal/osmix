{
	"$schema": "https://json.schemastore.org/package",
	"name": "osmix",
	"type": "module",
	"version": "0.0.6",
	"description": "Full OSM toolkit for JavaScript and TypeScript",
	"main": "./src/index.ts",
	"exports": {
		".": "./src/index.ts"
	},
	"publishConfig": {
		"access": "public",
		"main": "./dist/index.js",
		"types": "./dist/index.d.ts",
		"exports": {
			".": {
				"import": "./dist/index.js",
				"types": "./dist/index.d.ts"
			}
		},
		"files": [
			"dist",
			"README.md",
			"LICENSE"
		]
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/conveyal/osmix.git"
	},
	"homepage": "https://github.com/conveyal/osmix#readme",
	"bugs": {
		"url": "https://github.com/conveyal/osmix/issues"
	},
	"sideEffects": false,
	"scripts": {
		"build": "tsc",
		"prepublishOnly": "tsc",
		"test": "bun test",
		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"@types/bun": "catalog:",
		"@types/geojson": "catalog:",
		"typescript": "catalog:"
	},
	"dependencies": {
		"@osmix/change": "workspace:*",
		"@osmix/core": "workspace:*",
		"@osmix/geojson": "workspace:*",
		"@osmix/json": "workspace:*",
		"@osmix/pbf": "workspace:*",
		"@osmix/raster": "workspace:*",
		"@osmix/router": "workspace:*",
		"@osmix/shared": "workspace:*",
		"@osmix/vt": "workspace:*",
		"comlink": " ^4.4.2",
		"dequal": "catalog:"
	}
}
