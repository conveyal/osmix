{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "osmix",
	"type": "module",
	"version": "0.1.6",
	"description": "Full OSM toolkit for JavaScript and TypeScript",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/conveyal/osmix.git"
	},
	"exports": {
		".": {
			"import": "./src/index.ts",
			"default": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"release": "bun run build && bun pm pack --quiet && npm publish *.tgz --access=public --provenance=true",
		"test": "bun test",
		"typecheck": "tsgo --noEmit"
	},
	"devDependencies": {
		"@mapbox/tilebelt": "^2.0.3",
		"@types/bun": "catalog:",
		"@types/geojson": "catalog:",
		"typescript": "catalog:"
	},
	"dependencies": {
		"@osmix/change": "workspace:*",
		"@osmix/core": "workspace:*",
		"@osmix/geojson": "workspace:*",
		"@osmix/geoparquet": "workspace:*",
		"@osmix/gtfs": "workspace:*",
		"@osmix/json": "workspace:*",
		"@osmix/pbf": "workspace:*",
		"@osmix/raster": "workspace:*",
		"@osmix/router": "workspace:*",
		"@osmix/shapefile": "workspace:*",
		"@osmix/shared": "workspace:*",
		"@osmix/vt": "workspace:*",
		"comlink": " ^4.4.2",
		"dequal": "catalog:"
	}
}
