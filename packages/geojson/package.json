{
	"$schema": "https://json.schemastore.org/package",
	"name": "@osmix/geojson",
	"description": "Convert OSM files to and from GeoJSON.",
	"version": "0.0.11",
	"type": "module",
	"types": "./dist/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/conveyal/osmix.git"
	},
	"homepage": "https://github.com/conveyal/osmix#readme",
	"bugs": {
		"url": "https://github.com/conveyal/osmix/issues"
	},
	"sideEffects": false,
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"bench": "bun test --bench",
		"prepare": "bun run build",
		"release": "bun publish",
		"test": "bun test",
		"typecheck": "tsgo --noEmit"
	},
	"devDependencies": {
		"@types/bun": "catalog:",
		"@types/geojson": "catalog:",
		"typescript": "catalog:"
	},
	"dependencies": {
		"@osmix/core": "workspace:*",
		"@osmix/pbf": "workspace:*",
		"@osmix/shared": "workspace:*",
		"@placemarkio/geojson-rewind": "catalog:",
		"dequal": "catalog:"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"bun": "./src/index.ts",
			"development|production": "./src/index.ts",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"src",
		"README.md",
		"LICENSE"
	]
}
