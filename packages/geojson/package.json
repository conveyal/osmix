{
	"$schema": "https://json.schemastore.org/package",
	"name": "@osmix/geojson",
	"description": "Convert OSM files to and from GeoJSON.",
	"version": "0.0.5",
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"publishConfig": {
		"access": "public",
		"main": "./dist/index.js",
		"types": "./dist/index.d.ts",
		"exports": {
			".": {
				"import": "./dist/index.js",
				"types": "./dist/index.d.ts"
			}
		},
		"files": [
			"dist",
			"README.md",
			"LICENSE"
		]
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/conveyal/osmix.git"
	},
	"homepage": "https://github.com/conveyal/osmix#readme",
	"bugs": {
		"url": "https://github.com/conveyal/osmix/issues"
	},
	"sideEffects": false,
	"scripts": {
		"build": "tsc --excludeDirectories test",
		"bench": "bun test --bench",
		"prepublishOnly": "tsc",
		"test": "bun test",
		"typecheck": "tsgo --noEmit"
	},
	"devDependencies": {
		"@types/bun": "catalog:",
		"@types/geojson": "catalog:",
		"typescript": "catalog:"
	},
	"dependencies": {
		"@osmix/core": "workspace:*",
		"@osmix/pbf": "workspace:*",
		"@osmix/shared": "workspace:*",
		"@placemarkio/geojson-rewind": "catalog:",
		"dequal": "catalog:"
	}
}
