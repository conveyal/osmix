{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@osmix/geojson",
	"description": "Convert OSM files to and from GeoJSON.",
	"version": "0.0.11",
	"type": "module",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/conveyal/osmix.git"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"bench": "bun test --bench",
		"release": "bun run build && bun pm pack --quiet && npm publish *.tgz --access=public --provenance=true",
		"test": "bun test",
		"typecheck": "tsgo --noEmit"
	},
	"devDependencies": {
		"@types/bun": "catalog:",
		"@types/geojson": "catalog:",
		"typescript": "catalog:"
	},
	"dependencies": {
		"@osmix/core": "workspace:*",
		"@osmix/pbf": "workspace:*",
		"@osmix/shared": "workspace:*",
		"@placemarkio/geojson-rewind": "catalog:",
		"dequal": "catalog:"
	}
}
