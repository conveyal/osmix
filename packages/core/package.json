{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@osmix/core",
	"type": "module",
	"version": "0.1.6",
	"description": "An OSM PBF reader, writer, and merge tool written in TypeScript",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/conveyal/osmix.git"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"bench": "bun test --bench",
		"release": "bun run build && bun pm pack --quiet && npm publish *.tgz --access=public --provenance=true",
		"test": "bun test",
		"typecheck": "tsgo --noEmit"
	},
	"devDependencies": {
		"@osmix/pbf": "workspace:*",
		"@types/bun": "catalog:",
		"typescript": "catalog:"
	},
	"dependencies": {
		"@osmix/shared": "workspace:*",
		"flatbush": "^4.4.1",
		"geoflatbush": "^2.1.0",
		"geokdbush": "^2.0.1",
		"kdbush": "mourner/kdbush#16416cd"
	}
}
