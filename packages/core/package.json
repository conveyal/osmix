{
	"$schema": "https://json.schemastore.org/package",
	"name": "@osmix/core",
	"type": "module",
	"version": "0.1.5",
	"description": "An OSM PBF reader, writer, and merge tool written in TypeScript",
	"publishConfig": {
		"access": "public"
	},
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"bun": "./src/index.ts",
			"development|production": "./src/index.ts",
			"default": "./dist/index.js"
		},
		"./test/*": "./test/*.ts"
	},
	"files": [
		"dist",
		"src",
		"test",
		"README.md",
		"LICENSE"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/conveyal/osmix.git"
	},
	"homepage": "https://github.com/conveyal/osmix#readme",
	"bugs": {
		"url": "https://github.com/conveyal/osmix/issues"
	},
	"sideEffects": false,
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"bench": "bun test --bench",
		"prepare": "bun run build",
		"release": "bun publish",
		"test": "bun test",
		"typecheck": "tsgo --noEmit"
	},
	"devDependencies": {
		"@osmix/pbf": "workspace:*",
		"@types/bun": "catalog:",
		"typescript": "catalog:"
	},
	"dependencies": {
		"@osmix/shared": "workspace:*",
		"flatbush": "^4.4.1",
		"geoflatbush": "^2.1.0",
		"geokdbush": "^2.0.1",
		"kdbush": "mourner/kdbush#16416cd"
	}
}
