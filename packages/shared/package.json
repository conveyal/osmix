{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@osmix/shared",
	"type": "module",
	"version": "0.0.11",
	"description": "Shared utilities for Osmix",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/conveyal/osmix.git"
	},
	"exports": {
		"./tsconfig/base.json": "./tsconfig/base.json",
		"./tsconfig/test.json": "./tsconfig/test.json",
		"./tsconfig/app.json": "./tsconfig/app.json",
		"./*": "./src/*.ts"
	},
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"bench": "bun test --bench",
		"release": "bun run build && bun pm pack --quiet && npm publish *.tgz --access=public --provenance=true",
		"test": "bun test",
		"typecheck": "tsgo --noEmit"
	},
	"devDependencies": {
		"@types/bun": "catalog:",
		"typescript": "catalog:"
	},
	"dependencies": {
		"@mapbox/tilebelt": "^2.0.3",
		"dequal": "catalog:",
		"lineclip": "^2.0.0"
	}
}
