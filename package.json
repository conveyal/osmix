{
	"$schema": "https://json.schemastore.org/package",
	"name": "osmix",
	"packageManager": "bun@1.3.1",
	"private": true,
	"workspaces": {
		"packages": [
			"apps/*",
			"packages/*"
		],
		"catalog": {
			"dequal": "^2.0.3",
			"@mapbox/sphericalmercator": "^2.0.2",
			"maplibre-gl": "^5.9.0",
			"pbf": "^4.0.1",
			"typescript": "^5.9.0",
			"@types/geojson": "^7946.0.16",
			"@types/node": "^24",
			"@types/bun": "^1.3.1",
			"vite": "^7.1.7",
			"vitest": "^4.0.1"
		}
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.2",
		"@changesets/cli": "^2.29.7",
		"typescript": "catalog:"
	},
	"scripts": {
		"dev": "bun --filter '*' dev",
		"build": "bun --filter '*' build",
		"check": "biome check --write .",
		"format": "biome format --write .",
		"lint": "biome lint --write --unsafe .",
		"test": "bun --filter '*' test --silent",
		"typecheck": "bun --filter '*' typecheck"
	},
	"trustedDependencies": [
		"@tailwindcss/oxide"
	]
}
